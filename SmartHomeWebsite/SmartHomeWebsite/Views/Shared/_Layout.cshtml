
<!DOCTYPE html>

<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link href="@Url.Content("~/Content/images/icon-smart-house.png")" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css/style.css")
    @Scripts.Render("~/bundles/jquery")
   
</head>

<body>
    <div id="Header">
        <h1 style="color:#fff;  position:absolute;margin:auto; margin-left:210px; font-family:Georgia;"><center><img src="@Url.Content("~/Content/images/Home Filled-100.png")" style="width:50px;height:40px; padding-right:3px;" /><strong>Smart Home Systems</strong></center></h1>

    </div>
       
   
    <div id="Content">
      @RenderBody()
    </div>
    <div id="Footer"><center ><strong style="font-family:Georgia; ">&copy; @DateTime.Now.Year GeoTek BS MMC</strong></center> </div>
 

</body>


<script type="text/javascript">
    $(document).ready(function () {
        $('.switch').click(function () {

            $(this).toggleClass("switchOn")

        })
    });


   

</script>
<script>


</script>
</html>
